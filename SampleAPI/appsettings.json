{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YourSecretKeyForJWTTokenGeneration123456789",
    "Issuer": "SampleAPI",
    "Audience": "SampleAPIUsers",
    "ExpirationMinutes": 60
  },
  "AWS": {
    "Region": "ap-northeast-1",
    "SecretsManager": {
      "SecretName": "SampleAPI/Database"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=SampleDB;Integrated Security=true;"
  },
  "ExternalApi": {
    "BaseUrl": "https://api.example.com",
    "Timeout": 30
  }
}
